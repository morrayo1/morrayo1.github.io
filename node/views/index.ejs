    <% if(a>1){%>
        <div class="box_<%=a %>">Main</div>
    <%}%>
    -->
    
    <ul class="nemo">
        <%for(var i=0;i<color.length;i++){%>
            <li class="<%=color[i]%>"></li>
        <%}%>
<!--
        <li class="red"></li>
        <li class="blue"></li>
        <li class="green"></li>
-->
    </ul>
    
    
    
    <a href="/blog">블로그로 가기</a>
</body>

</html>